{
  "manifest_version": 3,
  "name": "TokenLogin",
  "description": "Login to your discord account via token!",
  "version": "1.0",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/main.js"
      ],
      "matches": [
        "https://discord.com/*",
        "https://discord.gg/*"
      ]
    }
  ]
}